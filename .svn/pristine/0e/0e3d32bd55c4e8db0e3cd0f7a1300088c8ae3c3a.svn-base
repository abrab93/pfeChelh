<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" 
"http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"   
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:f="http://java.sun.com/jsf/composite"
      >
     
      
	<h:head>
	
	<ui:insert name="title" />
	
	
        <ui:insert name="style">

            <meta charset="utf-8" />
            <meta name="viewport" content="width=device-width, initial-scale=1.0" />
            <title>Free Bootstrap Admin Template : Binary Admin</title>
            <!-- BOOTSTRAP STYLES-->
            <link href=" #{request.contextPath}/template/css/bootstrap.css" rel="stylesheet" />
            <!-- FONTAWESOME STYLES-->
            <link href=" #{request.contextPath}/template/css/font-awesome.css" rel="stylesheet" />
            <!-- CUSTOM STYLES-->
            <link href=" #{request.contextPath}/template/css/custom.css" rel="stylesheet" />
            <!-- GOOGLE FONTS-->
            <link href='http://fonts.googleapis.com/css?family=Open+Sans' rel='stylesheet' type='text/css' />
            <!-- JQUERY SCRIPTS -->
            <script src=" #{request.contextPath}/template/js/jquery-1.10.2.js"></script>
            <!-- BOOTSTRAP SCRIPTS -->
            <script src=" #{request.contextPath}/template/js/bootstrap.min.js"></script>
            <!-- METISMENU SCRIPTS -->
            <script src=" #{request.contextPath}/template/js/jquery.metisMenu.js"></script>
            <!-- CUSTOM SCRIPTS -->
            <script src=" #{request.contextPath}/template/js/custom.js"></script>
            <!-- pour resoudre le conflit entre le jq de primeFaces et le jq importer apartir de la template -->
            <script type="text/javascript">
            jQuery.noConflict();
            </script>
        </ui:insert>
	 
	</h:head>  
    <h:body>
    
    <div id="wrapper">
		

			<ui:insert name="header" >
				<ui:include src="header.xhtml">
				</ui:include>
				
			</ui:insert>
			
		<ui:insert name="content" >
		   		<ui:include src="content.xhtml" />
		</ui:insert>
		   	
		   	
		   	
		   	<ui:insert name="footer" >
				<ui:include src="footer.xhtml">
				</ui:include>
				
			</ui:insert>
		   	
		
	</div> <!-- wrapper -->
  
    </h:body>
</html>